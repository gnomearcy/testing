                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".aa_and_roboguice.AARoboAct"
            android:label="@string/title_activity_aarobo" >
        </activity>
    </application>

</manifest>
